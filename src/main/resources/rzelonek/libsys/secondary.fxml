<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.DatePicker?>

<VBox alignment="CENTER"
 spacing="20.0"
  xmlns="http://javafx.com/javafx/8.0.171"
   xmlns:fx="http://javafx.com/fxml/1" 
   fx:controller="rzelonek.libsys.controlers.SecondaryController">
    <children>


        <HBox spacing="10">
            <Label text="Welcome" />
            <Label fx:id="username" />
        </HBox>

        <HBox spacing="10">
            <Label text="PROFILE TOOLS:" />
            <Button fx:id="rentist" text="RENTED BY ME" onAction="#openrentlist" />
            <Button fx:id="BOOKSButton" text="BOOKS" onAction="#openavbbookks" />
            <Button fx:id="secondaryButton" text="LOG-OUT" onAction="#switchToPrimary" />
        </HBox>

        <HBox spacing="10"  fx:id="managementTools">
            <Label text="MANAGMENT TOOLS:" />
            <Button fx:id="booklist" text="BOOK MANAGER" onAction="#openbooklist" />
            <Button fx:id="userlist" text="USER MANAGER" onAction="#userlist" />
        </HBox>
        
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
